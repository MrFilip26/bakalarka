<?xml version="1.0" encoding="UTF-8"?>
<robot xsi:noNamespaceSchemaLocation="moves.xsd">
  <constants>
    <constant name="strafe_leg_crouch" value="30"/>
    <constant name="strafe_leg_span" value="7"/>
    <constant name="strafe_step_length" value="15"/>
  </constants>

  <low_skills>
    <low_skill name="test_turn_right" firstPhase="turn_right_big_stance"/>
  </low_skills>

  <phases>
    <phase name="turn_right_big_stance" next="start_big_turning_right">
      <effectors>
        <rle3 end="strafe_leg_crouch+30"/>
        <lle3 end="strafe_leg_crouch+30"/>
        <rle4 end="-2*strafe_leg_crouch"/>
        <lle4 end="-2*strafe_leg_crouch"/>
        <rle5 end="strafe_leg_crouch"/>
        <lle5 end="strafe_leg_crouch"/>
        <rae1 end="-90"/>
        <lae1 end="-90"/>
        <rae2 end="-10"/>
        <lae2 end="10"/>
        <rae3 end="90"/>
        <lae3 end="-90"/>
        <rae4 end="45"/>
        <lae4 end="-45"/>
      </effectors>
      <duration>500</duration>
    </phase>
    <phase name="start_big_turning_right" next="start_big_turning_right3">
      <effectors>
        <he1 end="0" computed="headAngle1"/>
        <he2 end="0" computed="headAngle2"/>
        <rle2 end="-50"/>
        <lle2 end="0"/>
        <rle6 end="4"/>
        <lle6 end="0"/>
      </effectors>
      <duration>240</duration>
    </phase>
    <phase name="start_big_turning_right3" next="start_big_turning_right4">
      <effectors>
        <he1 end="0" computed="headAngle1"/>
        <he2 end="0" computed="headAngle2"/>
        <rle2 end="0"/>
        <lle2 end="3"/>
        <rle6 end="0"/>
        <lle6 end="0"/>
      </effectors>
      <duration>180</duration>
    </phase>
    <phase name="start_big_turning_right4" next="start_big_turning_right5">
      <effectors>
        <he1 end="0" computed="headAngle1"/>
        <he2 end="0" computed="headAngle2"/>
        <rle2 end="0"/>
        <lle2 end="5"/>
        <rle6 end="0"/>
        <lle6 end="-2"/>
      </effectors>
      <duration>160</duration>
    </phase>
    <phase name="start_big_turning_right5" next="start_big_turning_right"
           isFinal="true">
      <effectors>
        <he1 end="0" computed="headAngle1"/>
        <he2 end="0" computed="headAngle2"/>
        <rle2 end="0"/>
        <lle2 end="3"/>
        <rle6 end="0"/>
        <lle6 end="0"/>
      </effectors>
      <duration>100</duration>
      <finalize>stop_big_turning_right</finalize>
    </phase>
    <phase name="stop_big_turning_right" setFlagTrue="in_walk_position" isFinal="true">
      <effectors>
        <he1 end="0" computed="headAngle1"/>
        <he2 end="0" computed="headAngle2"/>
        <rle1 end="0"/>
        <lle1 end="0"/>
        <rle2 end="7"/>
        <lle2 end="-7"/>
        <rle3 end="55"/>
        <lle3 end="55"/>
        <rle4 end="-60"/>
        <lle4 end="-60"/>
        <rle5 end="30"/>
        <lle5 end="30"/>
        <rle6 end="-7"/>
        <lle6 end="7"/>
        <rae1 end="-90"/>
        <lae1 end="-90"/>
        <rae2 end="-10"/>
        <lae2 end="10"/>
        <rae3 end="90"/>
        <lae3 end="-90"/>
        <rae4 end="45"/>
        <lae4 end="-45"/>
      </effectors>
      <duration>400</duration>
      <finalize>stop_big_turning_right</finalize>
    </phase>
  </phases>
</robot>