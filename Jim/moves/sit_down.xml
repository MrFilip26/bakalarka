<?xml version="1.0" encoding="utf-8"?>
<robot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="moves.xsd">
  <constants>
    <!--konstanty sem-->
  </constants>
  <low_skills>
    <low_skill name="sit_down" firstPhase="crunch2">
      <type>fall</type>
      <author>Andrej Sedlacek</author>
      <extendedFromMove>sit_down2</extendedFromMove>
      <description>Player sits down. This move is effective, relatively stable and very fast.</description>
    </low_skill>
  </low_skills>
  <phases>
    <phase name="crunch2" next="spread_legs2">
      <effectors>
        <he1 end="0"/>
        <he2 end="0"/>
        <rae2 end="0"/>
        <lae2 end="0"/>
        <rae3 end="0"/>
        <lae3 end="0"/>
        <rae4 end="0"/>
        <lae4 end="0"/>
        <rle1 end="0"/>
        <lle1 end="0"/>
        <rle2 end="0"/>
        <lle2 end="0"/>
        <rle6 end="0"/>
        <lle6 end="0"/>
        <rae1 end="-30"/>
        <lae1 end="-30"/>
        <rle3 end="35"/>
        <lle3 end="35"/>
        <lle4 end="-45"/>
        <rle4 end="-45"/>
        <rle5 end="15"/>
        <lle5 end="15"/>
      </effectors>
      <duration>50</duration>
    </phase>
    <phase name="spread_legs2" next="spread_legs_wider2">
      <effectors>
        <lle1 end="-20"/>
        <rle1 end="-20"/>
        <lle2 end="10"/>
        <rle2 end="-10"/>
        <lle3 end="45"/>
        <rle3 end="45"/>
        <rle4 end="-60"/>
        <lle4 end="-60"/>
        <rle6 end="10"/>
        <lle6 end="-10"/>
      </effectors>
      <duration>100</duration>
    </phase>
    <phase name="spread_legs_wider2" next="spread_legs_wider_with_twist2">
      <effectors>
        <lle1 end="-20"/>
        <rle1 end="-20"/>
        <lle2 end="30"/>
        <rle2 end="-30"/>
        <lle3 end="50"/>
        <rle3 end="50"/>
        <lle4 end="-40"/>
        <rle4 end="-40"/>
        <lle5 end="0"/>
        <rle5 end="0"/>
        <rle6 end="20"/>
        <lle6 end="-20"/>
      </effectors>
      <duration>100</duration>
    </phase>
    <phase name="spread_legs_wider_with_twist2" next="bend_knees_twist_legs2">
      <effectors>
        <rle1 end="-35"/>
        <lle1 end="-35"/>
        <rle2 end="-35"/>
        <lle2 end="35"/>
        <lle3 end="55"/>
        <rle3 end="55"/>
        <lle5 end="-10"/>
        <rle5 end="-10"/>
        <rle6 end="35"/>
        <lle6 end="-35"/>
      </effectors>
      <duration>100</duration>
    </phase>
    <phase name="bend_knees_twist_legs2" next="stretch_knees2">
      <effectors>
        <lle1 end="-50"/>
        <rle1 end="-50"/>
        <rle2 end="-40"/>
        <lle2 end="40"/>
        <lle3 end="65"/>
        <rle3 end="65"/>
        <lle4 end="-55"/>
        <rle4 end="-55"/>
        <lle5 end="-40"/>
        <rle5 end="-40"/>
        <lle6 end="-30"/>
        <rle6 end="30"/>
        <!--<lle5 end="-20" />
        <rle5 end="-20" />
        <lle6 end="-40" />
        <rle6 end="40" />-->
      </effectors>
      <duration>100</duration>
    </phase>
    <phase name="stretch_knees2" next="wait2">
      <effectors>
        <rae1 end="-70"/>
        <lae1 end="-70"/>
        <lle1 end="-75"/>
        <rle1 end="-75"/>
        <lle2 end="20"/>
        <rle2 end="-20"/>
        <lle3 end="60"/>
        <rle3 end="60"/>
        <lle4 end="0"/>
        <rle4 end="0"/>
      </effectors>
      <duration>200</duration>
    </phase>
    <phase name="wait2" isFinal="true">
      <duration>1000</duration>
      <finalize>wait2</finalize>
    </phase>
  </phases>
</robot>