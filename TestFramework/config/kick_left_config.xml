      <duration from="0" to="0" step="0" /> <!-- ma tu byt, ked nechcem menit? -->
      <effectors> 
        <lle3 from="-4" to="4" step="2" />
        <lle4 from="-5" to="5" step="5" />
        <lle5 from="-6" to="6" step="2" /> <!-- toto by sa dalo zmenit na rozsah <-4, 4> -->
      </effectors> 
