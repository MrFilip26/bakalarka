     <phase name="kickit">
      <duration from="-40" to="10" step="10" /> 
      <effectors>  
        <lle4 from="-10" to="15" step="5" />
        <lle5 from="-4" to="4" step="2" />  <!-- toto by sa mozno dalo vypustit -->       
      </effectors> 
     </phase> 
   </phases> 
</config> 
